    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.14",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/multer": "^1.4.7",
    "@types/node": "^18.11.7",
    "@types/pg": "^8.11.13",
    "@types/request": "^2.48.8",
    "@types/supertest": "^6.0.3",
    "concurrently": "^7.5.0",
    "jest": "^29.7.0",
    "nodemon": "^2.0.20",
    "sequelize-cli": "^6.6.2",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "typescript": "^4.9.5"
  }
}
