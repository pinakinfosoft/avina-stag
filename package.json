{
  "name": "tcctech",
  "version": "1.0.0",
  "description": "",
  "main": "index.ts",
  "engines": {
    "node": ">=18.x"
  },
  "scripts": {
    "build": "rimraf dist && tsc",
    "dev": "concurrently \"npx tsc --watch \" \" nodemon -q build/index.js\"",
    "stag": "concurrently \"npx tsc --watch \" \"export NODE_ENV=staging && nodemon -q build/index.js\"",
    "prod": "concurrently \"npx tsc --watch \" \"export NODE_ENV=production && nodemon -q build/index.js\"",
    "nungu": "npm run build && \"export NODE_ENV=nungu && nodemon -q build/index.js\"",
    "start": "nodemon -q build/index.js",
    "royale": "npm run build && \"export NODE_ENV=royale && nodemon -q build/index.js\"",
    "zamles": "npm run build && \"export NODE_ENV=zamles && nodemon -q build/index.js\"",
    "seed:pending": "node scripts/run-seeders.js",
    "seed:all": "node scripts/run-seeders.js --all",
    "seed:only": "node scripts/run-seeders.js --only",
    "seed:undo": "node scripts/undo-all-or-specific-remaing-seeder.js",
    "seed:undo:one": "node scripts/undo-all-or-specific-remaing-seeder.js",
    "db:create": "node src/version-4/config/createDatabase.ts",
    "migrate:clients": "node src/version-4/config/run-migrations.js",
    "migrate": "npm run db:create && sequelize-cli db:migrate --config src/version-4/config/config.js && npm run migrate:clients",
    "migrate:all": "npm run db:create && sequelize-cli db:migrate:all --config src/version-4/config/config.js",
    "migrate:undo": "npm run db:create && sequelize-cli db:migrate:undo --config src/version-4/config/config.js",
    "migrate:undo:all": "npm run db:create && sequelize-cli db:migrate:undo:all --config src/version-4/config/config.js && ",
    "test": "jest --detectOpenHandles --forceExit"
  },
  "author": "TCC TECH",
  "license": "ISC",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.500.0",
    "@aws-sdk/s3-request-presigner": "^3.500.0",
    "@paypal/checkout-server-sdk": "^1.0.3",
    "@types/html-pdf": "^3.0.0",
    "@types/puppeteer": "^7.0.4",
    "@types/wtfnode": "^0.7.3",
    "@woocommerce/woocommerce-rest-api": "^1.0.1",
    "axios": "^1.3.6",
    "bcrypt": "^5.1.0",
    "cluster": "^0.7.7",
    "compression": "^1.8.0",
    "cors": "^2.8.5",
    "crypto-js": "^4.1.1",
    "csv-parser": "^3.0.0",
    "dotenv": "^16.0.3",
    "expose-gc": "^1.0.0",
    "express": "^4.18.2",
    "express-validator": "^6.14.2",
    "form-data": "^4.0.4",
    "handlebars": "^4.7.7",
    "html-pdf": "^3.0.1",
    "image-to-webp": "^1.0.6",
    "jsonwebtoken": "^8.5.1",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^3.0.3",
    "nodemailer": "^6.9.1",
    "os": "^0.1.2",
    "pg": "^8.9.0",
    "puppeteer": "^24.8.2",
    "razorpay": "^2.9.4",
    "read-excel-file": "^5.6.1",
    "request": "^2.88.2",
    "sequelize": "^6.28.0",
    "sequelize-typescript": "^2.1.5",
    "sharp": "^0.33.5",
    "shopify-api-node": "^3.12.6",
    "stripe": "^14.22.0",
    "umzug": "^3.8.2",
    "wtfnode": "^0.9.1"
  },
